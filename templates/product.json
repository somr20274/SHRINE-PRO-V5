        },
        "collapsible_row_tmk4wz": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Does it replace collagen supplements?",
            "icon": "check_box",
            "filled_icon": false,
            "row_content": "<p>No. This is a topical formula designed to support the appearance of firmness on the skin’s surface.</p>",
            "page": ""
          }
        },
        "collapsible_row_NWPtfW": {
          "type": "collapsible_row",
          "settings": {
            "heading": "How often should I use it?",
            "icon": "check_box",
            "filled_icon": false,
            "row_content": "<p>For best results, apply once or twice daily on clean, dry skin and gently massage until absorbed.</p>",
