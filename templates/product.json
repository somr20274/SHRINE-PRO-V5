        },
        "collapsible_row_zyqXjG": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Is it suitable for sensitive skin?",
            "icon": "check_box",
            "filled_icon": false,
            "row_content": "<p><br/>The formula is gentle and free from harsh ingredients. However, we always recommend a patch test before full application</p>",
            "page": ""
