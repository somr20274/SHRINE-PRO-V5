        "collapsible_row_dV6Hny": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Can I use it on my face?",
            "icon": "check_box",
            "filled_icon": false,
            "row_content": "<p>Yes. The spray is suitable for the face, neck, and body.<br/><br/>Avoid direct contact with eyes and perform a patch test if you have sensitive skin.</p>",
